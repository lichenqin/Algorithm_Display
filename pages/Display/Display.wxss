.container{
  background-color: #fff;
  height: 40vh;
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
   align-items: flex-end;
  justify-content: space-around; 
}

.object{
  color: #008000;
  background-color:#008000; 
  width: 40rpx;
  height: 100rpx;
  border: solid;
}

.canvaStyle{
  background-color:#eee;
  width: 750rpx;
  height: 60vh; 
}

.formContbutton.submitBtn{
margin-top:60rpx;
width:100%;
height:100rpx;
display:flex;
justify-content:center;
align-items:center;
font-size:32rpx;
font-weight:bold;
}

.formContbutton.active{
color:#fff !important;
background:#ff9537 !important;
}

.formContbutton.gray{
color:#fff !important;
background:#d9d9d9 !important;
}
