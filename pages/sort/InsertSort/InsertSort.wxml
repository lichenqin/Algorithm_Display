<canvas canvas-id="insert" class="sort_canvaStyle"></canvas>
  <!-- 当使用绝对定位时，文档流后边的 canvas 的显示层级高于前边的 canvas -->
<view class='sliderContainer'>
  <view  class='sliderStyle'>
    <slider name='insertSlider' step='10' bindchange='listen_slider'></slider>
  </view>
</view>
<view class='sort_container'>
  <button bindtap='Process'>Insert sort</button>
  <button bindtap='reset'>Reset</button>
  <button bindtap='explain'>Explain</button>
</view>