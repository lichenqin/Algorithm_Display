  <swiper indicator-dots='true'>
    <swiper-item>
      <view class="middle" bindtap='sort'>

          <image src="/images/IndexArrayIcon.png" class="image-middle"></image>

      </view>
    </swiper-item>


    <swiper-item>
      <view class="middle"  bindtap='linklist'>

        <image src="/images/IndexLinkIcon.png" class="image-middle"></image>
      
      </view>
    </swiper-item>


    <swiper-item>
      <view class="middle" bindtap='BTree'>
     
          <image src="/images/INdexBTreeIcon.png" class="image-middle"></image>

      </view>

    </swiper-item>
</swiper>
